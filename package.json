    "@types/bcrypt": "^3.0.0",
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.2",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
